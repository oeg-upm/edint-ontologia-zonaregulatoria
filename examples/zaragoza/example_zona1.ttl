@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix esadm: <http://vocab.linkeddata.es/datosabiertos/def/sector-publico/territorio#> .
@prefix edintzone: <http://vocab.linkeddata.es/datosabiertos/def/common/zone#> .

# Custom Resource Prefixes
@prefix ex-zona: <http://example.org/resource/zona/> .
@prefix ex-via: <http://example.org/resource/via/> .
@prefix ex-org: <http://example.org/resource/organization/> .

#################################################################
#    Regulatory Zone 1 - ESRO (Rotation / Blue Zone)
#################################################################

ex-zona:1-esro a edintzone:ShortTermParkingZone ;
    rdfs:label "Zona 1 - ESRO (Estacionamiento Rotación)"@es ;
    dc:description "Área de Zona 1 destinada a estacionamiento de rotación (zona azul). Máximo 2 horas."@es ;
    
    # Common Properties
    edintzone:locatedIn <http://dbpedia.org/resource/Zaragoza> ;
    edintzone:managedBy ex-org:z-m-ute ;
    edintzone:scheduleDescription "Lunes a Viernes de 09:00 a 14:00 y de 17:00 a 20:00. Sábados, domingos y festivos sin regulación."^^xsd:string ;
    edintzone:legalReference <https://www.zaragoza.es/sede/servicio/normativa/13296#tit3-capitulo4> ;

    # Streets specifically listed as ESRO in Zona 1
    edintzone:composedOf ex-via:calle-eusebio-blasco ,
                         ex-via:calle-pedro-j-soler ,
                         ex-via:plaza-san-pedro-nolasco ,
                         ex-via:calle-san-andres ,
                         ex-via:calle-san-jorge ,
                         ex-via:calle-san-lorenzo ,
                         ex-via:calle-santo-dominguito-de-val ,
                         ex-via:calle-veronica .

#################################################################
#    Regulatory Zone 1 - ESRE (Residents / Orange Zone)
#################################################################

ex-zona:1-esre a edintzone:ResidentialParkingZone ;
    rdfs:label "Zona 1 - ESRE (Estacionamiento Residentes)"@es ;
    dc:description "Área de Zona 1 destinada prioritariamente a residentes (zona naranja)."@es ;
    
    # Common Properties
    edintzone:locatedIn <http://dbpedia.org/resource/Zaragoza> ;
    edintzone:managedBy ex-org:z-m-ute ;
    edintzone:scheduleDescription "Lunes a Viernes de 09:00 a 14:00 y de 17:00 a 20:00. Sábados, domingos y festivos sin regulación."^^xsd:string ;
    edintzone:legalReference <https://www.zaragoza.es/sede/servicio/normativa/13296#tit3-capitulo4> ;

    # Streets specifically listed as ESRE in Zona 1
    edintzone:composedOf ex-via:calle-mayor ,
                         ex-via:calle-refugio .

#################################################################
#    Municipality
#################################################################

<http://dbpedia.org/resource/Zaragoza> a esadm:Municipio ;
    rdfs:label "Zaragoza"@es ;
    # Linking both distinct zones to the municipality
    edintzone:hasRegulatedZone ex-zona:1-esro ,
                               ex-zona:1-esre .

#################################################################
#    Management Organization
#################################################################

ex-org:z-m-ute a org:Organization ;
    rdfs:label "Z+M UTE Servicio Estacionamiento Regulado de Zaragoza"@es ;
    rdfs:comment "Unión Temporal de Empresas encargada de la gestión del servicio."@es .

#################################################################
#    Streets (Vias)
#################################################################

# ESRO Streets
ex-via:calle-eusebio-blasco a esadm:Via ; rdfs:label "Calle Eusebio Blasco"@es .
ex-via:calle-pedro-j-soler a esadm:Via ; rdfs:label "Calle Pedro J. Soler"@es .
ex-via:plaza-san-pedro-nolasco a esadm:Via ; rdfs:label "Plaza San Pedro Nolasco"@es .
ex-via:calle-san-andres a esadm:Via ; rdfs:label "Calle San Andrés"@es .
ex-via:calle-san-jorge a esadm:Via ; rdfs:label "Calle San Jorge"@es .
ex-via:calle-san-lorenzo a esadm:Via ; rdfs:label "Calle San Lorenzo"@es .
ex-via:calle-santo-dominguito-de-val a esadm:Via ; rdfs:label "Calle Santo Dominguito de Val"@es .
ex-via:calle-veronica a esadm:Via ; rdfs:label "Calle Verónica"@es .

# ESRE Streets
ex-via:calle-mayor a esadm:Via ; rdfs:label "Calle Mayor"@es .
ex-via:calle-refugio a esadm:Via ; rdfs:label "Calle Refugio"@es .